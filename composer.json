{
    "name": "natsumework/laravel-cache-manager",
    "description": "Allows you to easily manage the cache with config file. And provide a simple solution to solve the cache penetration and hotspot invalid.",
    "keywords": [
        "laravel",
        "cache",
        "redis"
    ],
    "authors": [
        {
            "name": "natsumework",
            "email": "natsumework0902@gmail.com",
            "homepage": "https://github.com/natsumework",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "illuminate/cache": "^6.0 || ^7.0 || ^8.0",
        "illuminate/config": "^6.0 || ^7.0 || ^8.0",
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Natsumework\\CacheManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Natsumework\\CacheManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Natsumework\\CacheManager\\CacheManagerServiceProvider"
            ]
        }
    }
}
